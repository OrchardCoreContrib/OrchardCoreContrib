<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<VersionPrefix>1.4.0</VersionPrefix>
		<Authors>The Orchard Core Contrib Team</Authors>
		<Company />
		<Description>Provides ability to query using LINQ in Orchard Core data source.</Description>
		<PackageLicenseExpression>BSD-3-Clause</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/OrchardCoreContrib/OrchardCoreContrib/tree/main/src/OrchardCoreContrib.Linq/README.md</PackageProjectUrl>
		<RepositoryUrl>https://github.com/OrchardCoreContrib/OrchardCoreContrib</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PublishRepositoryUrl>true</PublishRepositoryUrl>
		<PackageTags>Orchard Core, Orchard Core Contrib, LINQ</PackageTags>
		<PackageReleaseNotes>https://github.com/OrchardCoreContrib/OrchardCoreContrib/releases</PackageReleaseNotes>
		<PackageId>OrchardCoreContrib.Linq</PackageId>
		<PackageIcon>icon.png</PackageIcon>
		<Product>Orchard Core Contrib LINQ</Product>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
		<Copyright>2019 Orchard Core Contrib</Copyright>
	</PropertyGroup>

	<ItemGroup>
		<None Include="../../images/icon.png" Pack="true" PackagePath="icon.png" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="OrchardCore.Autoroute" />
		<PackageReference Include="OrchardCore.Alias" />
		<PackageReference Include="OrchardCore.Users.Core" />
		<PackageReference Include="OrchardCore.AdminDashboard" />
		<PackageReference Include="OrchardCore.AuditTrail.Abstractions" />
		<PackageReference Include="OrchardCore.Deployment" />
		<PackageReference Include="OrchardCore.Layers" />
		<PackageReference Include="OrchardCore.Lists" />
		<PackageReference Include="OrchardCore.PublishLater" />
		<PackageReference Include="OrchardCore.Taxonomies" />
		<PackageReference Include="OrchardCore.Workflows" />
		<PackageReference Include="OrchardCore.OpenId.Core" />
		<PackageReference Include="OrchardCore.ContentManagement" />
		<PackageReference Include="OrchardCore.ContentLocalization.Abstractions" />
		<PackageReference Include="linq2db" />
	</ItemGroup>

</Project>
